{
    "manifest_version": 3,
    "name": "Custom Video Playback Extension",
    "version": "1.0",
    "permissions": ["activeTab", "scripting", "tabs"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
      ]
  }
  